                 xmlns:f="http://java.sun.com/jsf/core"
                 xmlns:ui="http://java.sun.com/jsf/facelets"
                 xmlns:p="http://primefaces.org/ui"
                 template="/template/mainTemplate.xhtml"
                 xmlns:x="http://xpert.com/faces">
    <ui:param name="title" value="Remuneração Dia a Dia" />
    <ui:define name="body">
        <style>
            .ui-datatable td, .ui-datatable thead > tr > th{white-space: nowrap !important; overflow: hidden;}
            .ui-datatable-scrollable table, .ui-datatable-sticky table { table-layout: fixed !important;}
            .ui-datatable-frozenlayout-left {width: 20%;}
        </style>
        <h:form  id="formDiaDia">
            <x:modalMessages autoUpdate="true"/>
            <p:remoteCommand name="rc" 
                             process="@form @this,tabView:tabConsulta" 
                             update=":formDiaDia:tabView:panelTabela1, :formDiaDia:tabView:panelTabela2" 
                             actionListener="#{diaADiaMB.buscar}"  />
            <p:tabView id="tabView" onTabChange="rc()" >

                <p:tab title="Consulta" id="tabConsulta">     

                    <div class="container-fluid">
                        <div class="row">
                            <div class="form-group col-lg-2 col-md-6 col-sm-6 col-xs-12">
                                <h:outputLabel for="menus" value="Recebimento: " />
                                <h:selectOneMenu id="menus" value="#{diaADiaMB.menu}" styleClass="form-control" >
                                    <f:selectItem itemLabel="Todos"/>
                                    <f:selectItems value="#{diaADiaMB.menus}" 
                                                   var="menu"
                                                   itemLabel="#{menu}"/>
                                </h:selectOneMenu>
                            </div>

                            <div class="form-group col-lg-2 col-md-6 col-sm-6 col-xs-12">
                                <h:outputLabel value="Periodo:"/>
                                <p:inputMask mask="99/9999" value="#{diaADiaMB.periodo}" styleClass="form-control">
                                </p:inputMask>
                            </div>
                            <div class="form-group col-lg-2 col-md-6 col-sm-6 col-xs-12">
                                <h:outputLabel for="grupo" value="Grupo de Usuário:" />
                                <h:selectOneMenu id="grupo" value="#{diaADiaMB.listaLoja}" converter="entityConverter" styleClass="form-control" >
                                    <x:initializer/>
                                    <f:selectItem itemLabel="#{xmsg['select']}" />
                                    <f:selectItems value="#{findAllBean.get(classMB.listaLoja)}" 
                                                   var="grupo"
                                                   itemLabel="#{grupo}"/>
                                    <p:ajax update="dataTableLoja" process="@this" listener="#{diaADiaMB.selecionarListaLojas}"/>
                                </h:selectOneMenu>
                            </div>
                            <div class="form-group col-lg-2 col-md-6 col-sm-6 col-xs-12">
                                <h:outputLabel for="grupoLoja" value="Grupo de Loja" />
                                <h:selectOneMenu id="grupoLoja" value="#{diaADiaMB.grupo}" converter="entityConverter" styleClass="form-control" >
                                    <x:initializer/>
                                    <f:selectItem itemLabel="#{xmsg['select']}" />
                                    <f:selectItems value="#{findAllBean.get(classMB.grupoLoja)}" 
                                                   var="grupo"
                                                   itemLabel="#{grupo}"/>
                                    <p:ajax update="dataTableLoja" process="@this" listener="#{diaADiaMB.buscarLoja()}"/>
                                </h:selectOneMenu>
                            </div>
                        </div>

                        <div class="row">
                            <p:dataTable paginator="true"  emptyMessage="#{xmsg['noRecordFound']}"
                                         var="loja" rowIndexVar="index" id="dataTableLoja" widgetVar="dataTableLojaWidget" styleClass="table-responsive"
                                         value="#{diaADiaMB.lojas}" selection="#{diaADiaMB.lojasSelecionadas}" rowKey="#{loja.id}" lazy="false" 
                                         >
                                <f:facet name="header">
                                    Seleção de Lojas
                                </f:facet>
                                <p:column selectionMode="multiple" style="width:16px;text-align:center"/>

                                <p:column headerText="#{msg['loja.id']}" style="text-align: center;" width="8%" 
                                          filterBy="#{loja.id}">
                                    <h:outputText value="#{loja.id}"/>
                                </p:column>

                                <p:column headerText="#{msg['loja.nome']}" filterBy="#{loja.nome}">
                                    <h:outputText value="#{loja.nome}"/>
                                </p:column>
                                <p:column headerText="Grupo Loja" filterBy="#{loja.grupoLoja.nome}" 
                                          style="text-align: left;" width="15%" >
                                    <h:outputText value="#{loja.grupoLoja}">
                                        <x:initializer/>
                                    </h:outputText>
                                </p:column>
                                <p:column headerText="Tipo Loja" filterBy="#{loja.tipoLoja.nome}" 
                                          style="text-align: left;" width="8%" >
                                    <h:outputText value="#{loja.tipoLoja}">
                                        <x:initializer/>
                                    </h:outputText>
                                </p:column>

                                <p:column headerText="#{msg['loja.ativo']}" style="text-align: center;"
                                          filterBy="#{loja.ativo}" filterOptions="#{booleanSelectItensEmptyOption}" width="8%">
                                    <h:outputText value="#{loja.ativo}" converter ="yesNoConverter" />
                                </p:column>


                            </p:dataTable>

                        </div>
                    </div> 

                </p:tab>
