<ui:composition  xmlns="http://www.w3.org/1999/xhtml"
                 xmlns:h="http://java.sun.com/jsf/html"
                 xmlns:f="http://java.sun.com/jsf/core"
                 xmlns:ui="http://java.sun.com/jsf/facelets"
                 xmlns:p="http://primefaces.org/ui"
                 template="/template/mainTemplate.xhtml"
                 xmlns:x="http://xpert.com/faces">
    <ui:define name="body">

        <h:form  id="formDiaDia">
            <x:modalMessages autoUpdate="true"/>
            <p:remoteCommand name="rc" 
                             process="@form @this,tabView:tabConsulta" 
                             update=":formDiaDia:tabView:panelTabela1, :formDiaDia:tabView:panelTabela2" 
                             actionListener="#{diaADiaMB.buscar}"  />
            <p:tabView id="tabView" onTabChange="rc()" >

                <p:tab title="Consulta" id="tabConsulta">     

                    <div class="container-fluid">
                        <div class="row">
                            <div class="form-group col-lg-4 col-md-6 col-sm-6 col-xs-12">
                                <p:outputLabel for="menus" value="Recebimento: " />
                                <p:selectOneMenu id="menus" value="#{diaADiaMB.menu}" styleClass="form-control" >
                                    <f:selectItem itemLabel="Todos"/>

                                    <f:selectItems value="#{diaADiaMB.menus}" />
                                </p:selectOneMenu>
                            </div>
                            <div class="form-group col-lg-4 col-md-6 col-sm-6 col-xs-12">
                                <h:outputLabel value="Periodo:"/>
                                <p:inputMask mask="99/9999" value="#{diaADiaMB.periodo}" styleClass="form-control">
                                </p:inputMask>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-lg-4 col-md-6 col-sm-6 col-xs-12">
                                <h:outputLabel value="Loja (Código ou Nome):"/>
                                <p:inputText size="255" value="#{diaADiaMB.codNome}" styleClass="form-control">
                                    <p:ajax event="blur" listener="#{diaADiaMB.buscarLoja}" process="@this" 
                                            update=":formDiaDia:tabView:dataTableLoja"/>
                                </p:inputText>
                            </div>
                            <div class="form-group col-lg-4 col-md-6 col-sm-6 col-xs-12">
                                <h:outputLabel for="tipo" value="Tipo de Loja:" />
                                <h:selectOneMenu id="tipo" value="#{diaADiaMB.tipoLoja}" converter="entityConverter" styleClass="form-control" >
                                    <x:initializer/>
                                    <f:selectItem itemLabel="Todos" />
                                    <f:selectItems value="#{findAllBean.get(classMB.tipoLoja)}" 
                                                   var="tipo"
                                                   itemLabel="#{tipo}"/>
                                    <p:ajax listener="#{diaADiaMB.buscarLoja}" process="@this" update=":formDiaDia:tabView:dataTableLoja"/>
                                </h:selectOneMenu>
                            </div>

                            <div class="form-group col-lg-4 col-md-6 col-sm-6 col-xs-12">
                                <h:outputLabel for="ativo" value="Ativo:" />
                                <h:selectOneMenu id="ativo" value="#{diaADiaMB.ativo}"  styleClass="form-control" >
                                    <f:selectItem itemLabel="Todos" />
                                    <f:selectItems value="#{findAllBean.get(classMB.ativo)}" 
                                                   var="ativo"
                                                   itemLabel="#{ativo.descricao}"/>

                                    <p:ajax listener="#{diaADiaMB.buscarLoja}" process="@this" update=":formDiaDia:tabView:dataTableLoja"/>

                                </h:selectOneMenu>
                            </div>
                        </div>
                        <div class="row">
                            <p:dataTable paginator="true"  emptyMessage="#{xmsg['noRecordFound']}"
                                         var="loja" rowIndexVar="index" id="dataTableLoja" widgetVar="dataTableLojaWidget" styleClass="table-responsive"
                                         value="#{diaADiaMB.lojas}" selection="#{diaADiaMB.lojasSelecionadas}" rowKey="#{loja.id}" lazy="false" 
                                         >
                                <f:facet name="header">
                                    Seleção de Lojas
                                </f:facet>
                                <p:column selectionMode="multiple" style="width:16px;text-align:center"/>

                                <p:column headerText="#{msg['loja.id']}" style="text-align: center;" width="8%" >
                                    <h:outputText value="#{loja.id}"/>
                                </p:column>

                                <p:column headerText="#{msg['loja.nome']}">
                                    <h:outputText value="#{loja.nome}"/>
                                </p:column>

                                <p:column headerText="#{msg['loja.ativo']}" style="text-align: center;">
                                    <h:outputText value="#{loja.ativo}" converter ="yesNoConverter" />
                                </p:column>


                            </p:dataTable>

                        </div>
                    </div> 

                </p:tab>
                <p:tab title="Quantidade" id="tab1">
