package br.com.jsoft.centralfinanceira.modelo.enums;

/**
 *
 * @author Juniel
 */
public enum CredDeb {
    
    CREDITO(1,"Crédito"),
    DEBITO(2,"Débito");

    private CredDeb(int num, String descricao) {
        this.descricao = descricao;
        this.num = num;
    }
    
    String descricao;
    int num;

    public String getDescricao() {
        return descricao;
    }

    public int getNum() {
        return num;
    }
}
