<ui:composition  xmlns="http://www.w3.org/1999/xhtml"
                 xmlns:h="http://java.sun.com/jsf/html"
                 xmlns:f="http://java.sun.com/jsf/core"
                 xmlns:ui="http://java.sun.com/jsf/facelets"
                 xmlns:p="http://primefaces.org/ui"
                 xmlns:x="http://xpert.com/faces">
    
  
    <h:form id="formCreateTabelaDeComissao" >
        <p:fieldset legend="#{xmsg['generalData']}">
            <x:modalMessages/>

            <div class="container-fluid">
                 <div class="row">
                      
                      <div class="form-group col-lg-4 col-md-6 col-sm-6 col-xs-12">
                         <h:outputLabel for="nome" value="* #{msg['tabelaDeComissao.nome']}:" />
                         <p:inputText id="nome" value="#{tabelaDeComissaoMB.entity.nome}" maxlength="40"  styleClass="form-control"  />
                     </div>
                      <div class="form-group col-lg-4 col-md-6 col-sm-6 col-xs-12">
                         <h:outputLabel for="percentualFixeo:input" value="#{msg['tabelaDeComissao.percentualFixeo']}:" />
                         <x:inputNumber id="percentualFixeo" value="#{tabelaDeComissaoMB.entity.percentualFixeo}" styleClass="form-control"/>
                     </div>

                 </div>
             </div>
        </p:fieldset>
        <h:outputText value="#{xmsg['requiredFieldsForm']}"/>
        <div class="uix-center">
            <x:securityArea rolesAllowed="tabelaDeComissao.create">
                <p:commandButton process="@form" update="@form" action="#{tabelaDeComissaoMB.save}" value="#{xmsg['save']}" />
            </x:securityArea>
            <x:securityArea rolesAllowed="tabelaDeComissao.audit">
                <x:audit for="#{tabelaDeComissaoMB.entity}"/>
            </x:securityArea>
        </div>
    </h:form>

</ui:composition>