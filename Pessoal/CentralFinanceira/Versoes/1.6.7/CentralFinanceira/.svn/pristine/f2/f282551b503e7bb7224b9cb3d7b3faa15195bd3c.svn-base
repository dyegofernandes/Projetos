package br.com.jsoft.centralfinanceira.vo;

import br.com.jsoft.centralfinanceira.modelo.cadastroBasicos.GrupoLoja;
import br.com.jsoft.centralfinanceira.modelo.cadastroBasicos.Loja;
import br.com.jsoft.centralfinanceira.modelo.enums.CredDeb;
import java.math.BigDecimal;

/**
 *
 * @author Juniel
 */
public class CreditoEDebitoVO {
    
    private Long id;
    
    private GrupoLoja grupoLoja;
    
    private String descricao;
    
    private BigDecimal valor;
    
    private CredDeb tipo;

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public GrupoLoja getGrupoLoja() {
        return grupoLoja;
    }

    public void setGrupoLoja(GrupoLoja grupoLoja) {
        this.grupoLoja = grupoLoja;
    }

    public String getDescricao() {
        return descricao;
    }

    public void setDescricao(String descricao) {
        this.descricao = descricao;
    }

    public BigDecimal getValor() {
        return valor;
    }

    public void setValor(BigDecimal valor) {
        this.valor = valor;
    }

    public CredDeb getTipo() {
        return tipo;
    }

    public void setTipo(CredDeb tipo) {
        this.tipo = tipo;
    }
    
    
}
