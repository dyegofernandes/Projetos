package br.com.weblicita.modelo.cadastro.enums;

/**
 *
 * @author Juniel
 */
public enum EditaPedido {

    ANEXADO(1, "Anexado em: XX / XX / XXXX"),
    AGUARDANDO(2, "Aguardando informações adicionais do órgão solicitante desde: XX / XX / XXXX"),
    FINALIZADO(3, "Finalizado em: XX / XX / XXXX");

    private EditaPedido(int num, String descricao) {
        this.descricao = descricao;
    }
    private String descricao;
    private int num;

    public String getDescricao() {
        return descricao;
    }

    public int getNum() {
        return num;
    }

}
