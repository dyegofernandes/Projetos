<ui:composition  xmlns="http://www.w3.org/1999/xhtml"
                 xmlns:h="http://java.sun.com/jsf/html"
                 xmlns:f="http://java.sun.com/jsf/core"
                 xmlns:ui="http://java.sun.com/jsf/facelets"
                 xmlns:p="http://primefaces.org/ui"
                 xmlns:x="http://xpert.com/faces">
    <h:form id="formDetailHomolagacaoAtaAdjucao" styleClass="uix-form-detail">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['homolagacaoAtaAdjucao.usuario']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{homolagacaoAtaAdjucaoMB.entity.usuario}" styleClass="uix-content-detail">
                        <x:initializer/>
                    </h:outputText>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['homolagacaoAtaAdjucao.pedidoLicitacao']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{homolagacaoAtaAdjucaoMB.entity.pedidoLicitacao}" styleClass="uix-content-detail">
                        <x:initializer/>
                    </h:outputText>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['homolagacaoAtaAdjucao.dataCriacao']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{homolagacaoAtaAdjucaoMB.entity.dataCriacao}" styleClass="uix-content-detail">
                        <f:convertDateTime />
                    </h:outputText>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['homolagacaoAtaAdjucao.ultimaGeracao']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{homolagacaoAtaAdjucaoMB.entity.ultimaGeracao}" styleClass="uix-content-detail">
                        <f:convertDateTime />
                    </h:outputText>
                </div>
           </div>
         </div>
        <p:separator/>
        <div class="uix-center">
            <p:commandButton type="button" value="#{xmsg['close']}" onclick="PF('widgetHomolagacaoAtaAdjucaoDetail').hide()" />
            <x:securityArea rolesAllowed="homolagacaoAtaAdjucao.audit">
                <x:audit for="#{homolagacaoAtaAdjucaoMB.entity}"/>
            </x:securityArea>
        </div>
    </h:form>
</ui:composition>