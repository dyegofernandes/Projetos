<ui:composition  xmlns="http://www.w3.org/1999/xhtml"
                 xmlns:h="http://java.sun.com/jsf/html"
                 xmlns:f="http://java.sun.com/jsf/core"
                 xmlns:ui="http://java.sun.com/jsf/facelets"
                 xmlns:p="http://primefaces.org/ui"
                 xmlns:x="http://xpert.com/faces">


    <h:form id="formFindFornecedor" >
        <x:modalMessages/>

        <div class="container-fluid">
            <div class="row">
                <div class="form-group col-lg-4 col-md-6 col-sm-6 col-xs-12" style="width:100%">
                    <div style="input-finds_left">
                        <x:cnpj id="cnpj" value="#{fornecedorMB.entity.cnpj}" placeholder="#{msg['fornecedor.find.cnpj']}" styleClass="form-control input-finds"  />
                    </div>
                    <p:commandButton process="@form" update="@form"  value="#{msg['fornecedor.find']}" class="button-finds" />
                </div>
            </div>
            <div class="row">
                <div class="form-group col-lg-4 col-md-6 col-sm-6 col-xs-12" style="width:100%">
                    <div class="div-button-new">
                        <p:commandButton process="@form" update="@form"  value="#{msg['fornecedor.new']}" class="button-new" />
                    </div>
                    <div class="div-button-edit">
                        <p:commandButton process="@form" update="@form"  value="#{msg['fornecedor.edit']}" class="button-edit" />
                    </div>
                    <div class="div-button-delete">
                        <p:commandButton process="@form" update="@form"  value="#{msg['fornecedor.delete']}" class="button-delete" />
                    </div>
                </div>
            </div>
        </div>
    </h:form>

</ui:composition>